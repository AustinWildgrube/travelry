{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "verbatimModuleSyntax": true,
    "paths": {
      "colors": [
        "./types/rneui.d.ts"
      ],
      "@env": [
        "./types/env.d.ts"
      ],
      "&/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
